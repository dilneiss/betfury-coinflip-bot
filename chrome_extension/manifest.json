{
  "manifest_version": 2,
  "name": "Bet Fury - ",
  "description": "This extension does something,",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "images/get_started128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/get_started16.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches":["https://betfury.io/*"],
      "js": ["jquery.min.js"]
    }
  ]
}